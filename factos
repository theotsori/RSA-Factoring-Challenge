#!/usr/bin/python3
import sys

def factorize(n):
    # Find the smallest factor of n
    for i in range(2, int(n ** 0.5) + 1):
        if n % i == 0:
            return i, n // i
    return n, 1

# Check if a file was provided as an argument
if len(sys.argv) != 2:
    print("Usage: factors <file>")
    sys.exit(1)

# Read the file and factorize each number
with open(sys.argv[1], "r") as f:
    for line in f:
        n = int(line.strip())
        p, q = factorize(n)
        print(f"{n}={p}*{q}")

